  background: linear-gradient(to bottom,  rgba(81,91,102,1) 0%,rgba(89,104,117,1) 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#515b66', endColorstr='#596875',GradientType=0 ); /* IE6-9 */
}



.background-gradient(){
  background: rgb(125,126,125); /* Old browsers */
  background: -moz-linear-gradient(top,  rgba(125,126,125,1) 0%, rgba(0,0,0,1) 7%, rgba(56,56,56,1) 96%, rgba(14,14,14,1) 100%); /* FF3.6+ */
