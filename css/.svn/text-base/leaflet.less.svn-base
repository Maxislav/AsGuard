.button(){
  background: rgb(129,152,155); /* Old browsers */
  background: -moz-linear-gradient(top,  rgba(129,152,155,1) 0%, rgba(103,103,109,1) 47%, rgba(10,14,10,1) 51%, rgba(10,8,9,1) 77%, rgba(93,102,101,1) 100%); /* FF3.6+ */
