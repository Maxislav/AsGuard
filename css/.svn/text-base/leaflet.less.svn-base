/* Tiny Scrollbar */
#scrollbar1 { width: 170px; margin: 20px 0 10px; height: 90%}
#scrollbar1 .viewport { width: 160px; height: 100%; overflow: hidden; position: relative; }
#scrollbar1 .overview { list-style: none; position: absolute; left: 0; top: 0; padding: 0; margin: 0; }
#scrollbar1 .scrollbar{ background: transparent url(../images/bg-scrollbar-track-y.png) no-repeat 0 0; position: relative; background-position: 0 0; float: right; width: 6px; }
/*#scrollbar1 .track { background: transparent url(../images/bg-scrollbar-trackend-y.png) no-repeat 0 100%; height: 100%; width:4px; position: relative; padding: 0 1px; }*/
#scrollbar1 .track { background-color:rgba(150, 150, 150, 0.6) ; width: 100%; height:10px; position: relative; border-radius: 3px;   }

/*#scrollbar1 .thumb { background: transparent url(../images/bg-scrollbar-thumb-y.png) no-repeat 50% 100%; height: 20px; width: 6px; cursor: pointer; overflow: hidden; position: absolute; top: 0; left: 0px; }*/
/*#scrollbar1 .thumb .end { background: transparent url(../images/bg-scrollbar-thumb-y.png) no-repeat 50% 0; overflow: hidden; height: 5px; width: 6px; }*/
#scrollbar1 .thumb .end { background-color:rgba(0, 0, 0, 0.0) ; overflow: hidden; height: 5px; width: 6px; border-radius: 2px 2px 0 0}
#scrollbar1 .thumb {
  background-color:rgba(0, 0, 0, 0.6) ;
  height: 10px; cursor: pointer; overflow: hidden;
  position: absolute; left: 0px; top: 0px; z-index:5;
  border-radius: 3px;
  box-shadow: 0 1px 1px rgba(0,0,0,0.6),0 -1px 1px rgba(0,0,0,0.6);
}
#scrollbar1 .disable { display: none; }


#scrollbar2 { width: 100%; margin: 20px 0 10px; height: 90%}
