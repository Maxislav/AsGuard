  background: -ms-linear-gradient(top,  rgba(125,126,125,1) 0%,rgba(0,0,0,1) 7%,rgba(56,56,56,1) 96%,rgba(14,14,14,1) 100%); /* IE10+ */
  background: linear-gradient(to bottom,  rgba(125,126,125,1) 0%,rgba(0,0,0,1) 7%,rgba(56,56,56,1) 96%,rgba(14,14,14,1) 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#7d7e7d', endColorstr='#0e0e0e',GradientType=0 ); /* IE6-9 */

}

