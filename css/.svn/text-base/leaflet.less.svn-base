#scrollbar4 .thumb { background: rgba(0,0,0,0.6); height: 20px; width: 10px; cursor: pointer; overflow: hidden; position: absolute; top: 0; left: 0px; border-radius: 5px;}
#scrollbar4 .thumb .end { background: rgba(0,0,0,0); overflow: hidden; height: 5px; width: 6px; border-radius: 4px 4px 0 0}
#scrollbar4 .disable { display: none; }
