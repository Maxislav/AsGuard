  background: -ms-linear-gradient(top,  rgba(129,152,155,1) 0%,rgba(103,103,109,1) 47%,rgba(10,14,10,1) 51%,rgba(10,8,9,1) 77%,rgba(93,102,101,1) 100%); /* IE10+ */
  background: linear-gradient(to bottom,  rgba(129,152,155,1) 0%,rgba(103,103,109,1) 47%,rgba(10,14,10,1) 51%,rgba(10,8,9,1) 77%,rgba(93,102,101,1) 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#81989b', endColorstr='#5d6665',GradientType=0 ); /* IE6-9 */


  border: @border-black;
  color: #ffffff;
  text-shadow: none;
}

.button-active(){
  background: rgb(81,91,102); /* Old browsers */
  background: -moz-linear-gradient(top,  rgba(81,91,102,1) 0%, rgba(89,104,117,1) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(81,91,102,1)), color-stop(100%,rgba(89,104,117,1))); /* Chrome,Safari4+ */
