
body{
    font-family:Arial;
    font-weight: bold;
    font-size: 14px;
    color: #000;
    text-shadow: 0px 1px 0px rgba(255,255,255,0.5);
    background: url(../images/index_map.jpg);background-attachment: fixed; background-size: 50% 50%;
}
*{
   margin: 0;
}
#instructionDiv1{
    background: -moz-linear-gradient(top,  rgba(0,0,0,0.93) 0%, rgba(130,140,149,0.93) 19%, rgba(0,0,0,0.93) 50%, rgba(116,125,127,0.94) 84%, rgba(0,0,0,0.94) 100%); /* FF3.6+ */
